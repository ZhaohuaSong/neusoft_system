{% extends "mainbase.html" %}
{% load staticfiles %}
{% load i18n staticfiles %}
{% load url from future %}
<html lang="en">
<head>
    <meta charset="UTF-8">
    {% block scripts %}

        <!-- page specific plugin scripts -->
        <link rel="stylesheet" href="{% static "css/jquery-ui.min.css" %}"/>
        <!-- jquery scripts -->
        <script src="{% static "js/jquery-ui.min.js" %}"></script>
        <script src="{% static "js/jquery.ui.touch-punch.min.js" %}"></script>
        <script src="{% static "js/jquery.dataTables.min.js" %}"></script>
        <script src="{% static "js/jquery.dataTables.bootstrap.min.js" %}"></script>
        <script src="{% static "js/dataTables.buttons.min.js" %}"></script>
        <!-- buttons scripts -->
        <script src="{% static "js/buttons.flash.min.js" %}"></script>
        <script src="{% static "js/buttons.html5.min.js" %}"></script>
        <script src="{% static "js/buttons.colVis.min.js" %}"></script>
        <!-- ace scripts -->
        <script src="{% static "js/ace-elements.min.js" %}"></script>
        <script src="{% static "js/ace.min.js" %}"></script>
        {#        <script type="text/javascript" charset="utf8" src="//cdn.datatables.net/1.10.12/js/jquery.dataTables.js"></script>#}
        <!-- txnname scripts -->
        <script type="text/javascript">
            var TXNNAMES_LIST_URL = '{% url "sysadmin:syslog_jsonlist" %}';
        </script>
        <script src="{% static "js/sysadmin/syslog_list.js" %}"></script>
    {% endblock %}
</head>
<body class="no-skin">
{% block content %}
    <div class="main-content">
        <div class="breadcrumbs" id="breadcrumbs">
            <script type="text/javascript">
                try {
                    ace.settings.check('breadcrumbs', 'fixed')
                } catch (e) {
                }
            </script>
            <ul class="breadcrumb">
                <li>
                    <i class="ace-icon fa fa-home home-icon"></i>
                    <a href="#">首页</a>
                </li>
                {% if 'opr' in request.path %}
                    <li><a href="#">日志管理</a></li>
                    <li class="active">操作日志</li>
                {% else %}
                    <li><a href="#">系统管理</a></li>
                    <li class="active">操作记录</li>
                {% endif %}
            </ul><!-- /.breadcrumb -->
        </div>

        <div class="page-content">
            <div class="row">
                <div class="col-xs-12">
                    {% include "page_search.html" %}<!-- /.page-header -->
                    <div class="clearfix">
                        <div class="pull-right tableTools-container"></div>
                    </div>
                    <div class="table-header" style="margin-top: 10px;"> <!--table-header -->
                        操作记录
                    </div>
                    <div>
                        <table id="dynamic-table" class="table table-striped table-bordered table-hover"><!--table -->
                            <thead class="thin-border-bottom">
                            <tr>


                                <th class="hidden-480" width='55px'>序号</th>
                                <th>用户名</th>
                                <th class="hidden-480">用户手机号</th>
                                <th class="hidden-480">角色</th>
                                <th>机构</th>
                                <th>操作资源url</th>
                                <th>请求参数</th>
                                <th>IP地址</th>
                                <th>操作时间</th>
                            </tr>
                            </thead>
                            <tbody>
                            </tbody>
                        </table><!--table -->
                    </div>
                </div><!-- /.span -->
            </div><!-- /.row -->
        </div>
    </div>
    {% include "message.html" %}
{% endblock %}
<!-- /.main-content -->
</body>
</html>
