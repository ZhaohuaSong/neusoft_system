                                <div class="widget-body" style="background:none;">
                                    <div class="widget-main">
                                        <div id="fuelux-wizard-container">

                                            <hr/>
                                            <div class="step-content pos-rel col-sm-12">

                                                <form class="form-horizontal" role="form" action="" id="validation-form"
                                                      method="post" enctype="multipart/form-data">
                                                    {% csrf_token %}
                                                    <h3 style="text-indent:1em;">请选择CSV/XLS/TXT文件！</h3>
                                                    <div class="step-pane active" data-step="1" id="form-field-subject">
                                                        {{ form.filename }}
                                                    </div>
                                                </form>
{#                                                    <form enctype="multipart/form-data" method="post" action="/filemanage/file/upload">#}
{#                                                        {% csrf_token %}#}
{#                                                       <input type="file" name="file" />#}
{#                                                       <br />#}
{#                                                       <input type="submit" value="上传文件" />#}
{#                                                    </form>#}
                                            </div>
                                            <hr/>
                                        </div>
                                        <div class="wizard-actions col-xs-12" style="float:right; margin-bottom:20px;">
                                            <button id="mybtn" class="btn btn-red btn-info btn-bold "
                                                    type="button"  onclick="save();"> 保存 <i
                                                    ></i></button>
                                        </div>
                                    </div>
                                    <!-- /.widget-main -->
                                </div>
                            </div>
                            <!-- PAGE CONTENT ENDS -->
                        </div>
                        <!-- /.col -->
                    </div>
                    <!-- /.row -->
                </div>
                <!-- /.page-content -->
            </div>
