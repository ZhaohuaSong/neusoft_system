#!/usr/bin/env python
# -*- coding: UTF-8 -*-

import os
import sys

'''
整合oracle,mysql业务触发进程
'''

def create_trigger_proc(trigger_id=123):
    result  =os.system("python -m richDataX-system.dbadmin.db_trigger_proc "+str(trigger_id))
    print result

if __name__ == '__main__':
    print 'Parent process %s.' % os.getpid()
    create_trigger_proc(sys.argv[1])


